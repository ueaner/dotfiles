<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>KeepAlive</key>
    <true/>
    <key>Label</key>
    <string>org.gofrp.frpc</string>
    <key>ProgramArguments</key>
    <array>
        <string>/opt/local/bin/frpc</string>
        <string>-c</string>
        <string>/opt/local/etc/frpc_mac15.toml</string>
    </array>
    <key>RunAtLoad</key>
    <true/>

    <key>StandardErrorPath</key>
    <string>/tmp/frpc.stderr.log</string>

    <key>StandardOutPath</key>
    <string>/tmp/frpc.stdout.log</string>
</dict>
</plist>
