<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
        <key>KeepAlive</key>
        <true/>
        <key>Label</key>
        <string>homebrew.shadowsocks-rust</string>
        <key>ProgramArguments</key>
        <array>
                <string>/usr/local/bin/ssservice</string>
                <string>local</string>
                <string>--log-without-time</string>
                <string>-c</string>
                <string>/usr/local/etc/shadowsocks-rust.json</string>
        </array>
        <key>RunAtLoad</key>
        <true/>
</dict>
</plist>
